<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">
	<Script file="AreaPOIDataProvider.lua"/>

	<Frame name="AreaPOIPinTemplate" hidden="true" flattenRenderLayers="true" frameStrata="HIGH" enableMouse="true" mixin="AreaPOIPinMixin" virtual="true">
		<Size x="50" y="50"/>

		<Layers>
			<Layer level="ARTWORK">
				<Texture parentKey="Texture">
					<Anchors>
						<Anchor point="CENTER" />
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="HIGHLIGHT">
				<Texture parentKey="Highlight" alphaMode="ADD">
					<Anchors>
						<Anchor point="CENTER" />
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
	</Frame>
</Ui>
